generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model guest {
  id           Int     @id @default(autoincrement())
  contact      String
  contact_type String?
  cname        String?
  name         String
  school       String
  cos          String?
  status       Int     @default(0)
}
